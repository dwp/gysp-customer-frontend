    "chai-as-promised": "7.1.1",
    "conventional-changelog-cli": "2.2.2",
    "del": "6.1.0",
    "eslint": "8.16.0",
    "gulp-better-rollup": "4.0.1",
    "gulp-clean": "0.4.0",
