    "sinon": "14.0.0",
    "snyk": "1.904.0"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": "8.9.0"
  },
  "scripts": {
    "postinstall": "husky install",
    "build": "gulp generateAssets",
    "package": "gulp --gulpfile ./gulp-package.js",
    "prestart": "npm ci",
    "start": "node -r dotenv/config start | bunyan",
    "compliance": "npm run compliance:lint && npm run compliance:lint-docker",
    "compliance:lint": "eslint --cache .",
    "compliance:lint-docker": "docker run --rm -i hadolint/hadolint < docker/Dockerfile",
    "test": "npm run test:unit",
    "test:unit": "mocha test/unit --recursive",
    "quality": "npm run quality:coverage",
    "quality:coverage": "nyc mocha --reporter=mocha test/unit --recursive",
    "security": "npm run security:audit && npm run security:outdated",
    "security:audit": "npm audit || true",
    "security:outdated": "npm outdated || true",
    "security:snyk": "snyk test || true"
  },
  "snyk": true
}
